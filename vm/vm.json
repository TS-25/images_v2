{
  "Image": "ghcr.io/jlesage/kvm:latest",
  "Cmd": "bash",
  "Env": {
    "START": "qemu-system-x86_64 -enable-kvm -m 2048 -cpu host -smp 2 -hda /vm/ubuntu.qcow2 -net nic -net user"
  },
  "StopCommand": "poweroff",
  "Name": "UbuntuVM",
  "Description": "Virtual Machine running Ubuntu inside Docker.",
  "Author": "skyport",
  "AuthorName": "static",
  "Meta": {
    "Version": "1.0"
  }
}
